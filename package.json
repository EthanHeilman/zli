{
  "name": "@bastionzero/zli",
  "version": "6.20.10",
  "description": "BastionZero cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastionzero/zli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": "dist/src/index.js",
  "engines": {
    "node": "^14",
    "npm": ">=6"
  },
  "scripts": {
    "prepare": "npx ts-patch install -s",
    "start": "ts-node src/index.ts",
    "build-binaries-macos": "npx tsc && cd bzero/bctl/daemon && cross-env GOOS=darwin GOARCH=amd64 go build",
    "build-binaries-linux": "npx tsc && cd bzero/bctl/daemon && cross-env GOOS=linux GOARCH=amd64 go build",
    "build-binaries-windows": "npx tsc && cd bzero/bctl/daemon && cross-env GOOS=windows GOARCH=amd64 go build",
    "update-daemon-version": "replace '\\$DAEMON_VERSION' \"$(cat bzero/VERSION)\" bzero/bctl/daemon/daemon.go",
    "release-dev": "tsc && npm run package",
    "release": "run-script-os",
    "release:linux": "npm run clean && npm run release-linux",
    "release:macos": "npm run clean && npm run release-macos",
    "release:windows": "npm run clean && npm run release-windows",
    "release-all": "npm run clean && npm run release-macos && npm run release-linux && npm run release-windows",
    "release-macos": "npm run build-binaries-macos && npm run copy-files && pkg . -t node14-macos-x64 -o bin/zli-macos",
    "release-linux": "npm run build-binaries-linux && npm run copy-files && pkg . -t node14-linux-x64 -o bin/zli-linux",
    "release-windows": "npm run build-binaries-windows && npm run copy-files && pkg . -t node14-win-x64 -o bin/zli.exe",
    "release-prod": "npm run update-daemon-version && npm run release-all",
    "copy-files": "copyfiles src/**/*.html src/**/*.css bzero/bctl/daemon/* dist/",
    "package": "npm run copy-files && pkg .",
    "clean": "rimraf dist/ && rimraf bin/",
    "test": "jest --config src/jest-config-unit-tests.js --verbose",
    "system-test": "jest --config src/system-tests/jest-config-system-tests.js --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" \"webshell-common-ts/**/*.ts\" -c webshell-common-ts/.eslintrc.json"
  },
  "author": "BastionZero",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/reporters": "^27.5.1",
    "@jest/test-result": "^27.5.1",
    "@types/adm-zip": "^0.5.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^27.5.0",
    "@types/node-forge": "^1.3.1",
    "@types/pg": "^8.6.5",
    "@types/proper-lockfile": "^4.1.2",
    "@types/uuid": "^8.3.4",
    "@types/winreg": "^1.2.31",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "aws-sdk": "^2.1229.0",
    "axios": "^0.26.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dots-wrapper": "^3.7.13",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "fast-check": "^3.3.0",
    "jest": "^27.3.1",
    "jest-2-testrail": "^1.0.9",
    "jest-mock-extended": "2.0.6",
    "leaked-handles": "^5.2.0",
    "mock-stdin": "^1.0.0",
    "pg": "^8.7.3",
    "pkg": "^5.3.1",
    "proper-lockfile": "^4.1.2",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^27.0.7",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.0.5",
    "typescript-transform-paths": "^3.4.6",
    "undefined": "^0.1.0"
  },
  "dependencies": {
