    "yaml": "^2.1.3",
    "yargs": "^17.5.1"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "node_modules/figlet/fonts/Standard.flf",
      "bzero/bctl/daemon/daemon",
      "bzero/bctl/daemon/daemon.exe"
    ],
    "outputPath": "bin",
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ]
  }
}
